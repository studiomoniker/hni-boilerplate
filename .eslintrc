{
    "rules": {
        "no-console": 1,
        "camelcase": 2,
        
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ]
    },
    "env": {
        "es6": true,
        "browser": true,
        "amd": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "extends": "eslint:recommended"
}