{
  "extends": "eslint:recommended",
  "env": {
    "es6": true,
    "browser": true,
    "amd": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-console": 1,
    "camelcase": 2,
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"]
  }
}
